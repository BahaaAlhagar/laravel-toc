{
    "name": "balhagar/laravel-toc",
    "description": "Laravel Table of contents based on Wordpress Table of contents plugin",
    "keywords": [
        "balhagar",
        "laravel-toc"
    ],
    "homepage": "https://github.com/balhagar/laravel-toc",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Bahaa Alhagar",
            "email": "bahaa.rock@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Balhagar\\LaravelToc\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Balhagar\\LaravelToc\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Balhagar\\LaravelToc\\LaravelTocServiceProvider"
            ],
            "aliases": {
                "LaravelToc": "Balhagar\\LaravelToc\\LaravelTocFacade"
            }
        }
    }
}
