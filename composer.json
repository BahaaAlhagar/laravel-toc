{
    "name": "balhagar/laravel-toc",
    "type": "library",
    "description": "Laravel Table of contents based on Wordpress Table of contents plugin",
    "keywords": [
        "balhagar",
        "BahaaAlhagar",
        "laravel-toc",
        "laravel-table-of-contents",
        "table-of-contents",
        "toc",
        "Laravel",
        "package"
    ],
    "homepage": "https://github.com/BahaaAlhagar/laravel-toc",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Bahaa Alhagar",
            "homepage": "https://github.com/BahaaAlhagar",
            "email": "bahaa.rock@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^5.4|^7.0|^8.0",
        "illuminate/support": "^5.0|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Balhagar\\LaravelToc\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Balhagar\\LaravelToc\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Balhagar\\LaravelToc\\TocServiceProvider"
            ],
            "aliases": {
                "Toc": "Balhagar\\LaravelToc\\Facades\\Toc"
            }
        }
    }
}
